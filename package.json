{
  "name": "@mloberg/daft-bot",
  "license": "MIT",
  "homepage": "https://github.com/mloberg/daft-discord-bot",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "server": "node -r dotenv/config dist/app/index.js",
    "dev": "concurrently -k -c cyan.bold,green.bold,yellow.bold npm:build:watch npm:bot:watch npm:server:watch",
    "build:watch": "npm run build -- --watch",
    "bot:watch": "nodemon --delay 2.5 -w dist --exec \"npm run --silent start\"",
    "server:watch": "nodemon --delay 2.5 -w dist --exec \"npm run --silent server\"",
    "pretest": "DATABASE_URL=\"postgresql://postgres:secret@127.0.0.1/test\" prisma db push --preview-feature --skip-generate --accept-data-loss",
    "test": "DATABASE_URL=\"postgresql://postgres:secret@127.0.0.1/test\" BOT_TOKEN=test SECRET=xxxxtestingxxsecretxxxx jest --detectOpenHandles --coverage --verbose",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint --cache .",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "@prisma/client": "^2.17.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.3",
    "pino": "^6.11.1",
    "prism-media": "^1.2.7",
    "yargs": "^16.2.0",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/nunjucks": "^3.1.4",
    "@types/pino": "^6.3.6",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "prisma": "^2.18.0",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
